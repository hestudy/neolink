{
  "name": "@neolink/api",
  "version": "0.1.0",
  "private": true,
  "description": "NeoLink API server built with Hono.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.2",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.0",
    "@neolink/ai": "workspace:*",
    "@neolink/database": "workspace:*",
    "@neolink/shared": "workspace:*",
    "@orpc/client": "^1.8.1",
    "@orpc/contract": "^1.8.1",
    "@orpc/server": "^1.8.1",
    "@orpc/shared": "^1.8.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "bcryptjs": "^3.0.2",
    "hono": "^4.9.2",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "redis": "^5.8.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@neolink/config": "workspace:*",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.55.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
